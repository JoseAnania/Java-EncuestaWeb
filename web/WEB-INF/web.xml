<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletListaRespuesta</servlet-name>
        <servlet-class>Servlets.ServletListaRespuesta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAltaRespuesta</servlet-name>
        <servlet-class>Servlets.ServletAltaRespuesta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletReporte1</servlet-name>
        <servlet-class>Servlets.ServletReporte1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletReporte2</servlet-name>
        <servlet-class>Servlets.ServletReporte2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletReporte3</servlet-name>
        <servlet-class>Servlets.ServletReporte3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletListaRespuesta</servlet-name>
        <url-pattern>/ServletListaRespuesta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAltaRespuesta</servlet-name>
        <url-pattern>/ServletAltaRespuesta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletReporte1</servlet-name>
        <url-pattern>/ServletReporte1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletReporte2</servlet-name>
        <url-pattern>/ServletReporte2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletReporte3</servlet-name>
        <url-pattern>/ServletReporte3</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
